section.cars
	each car in carsArr
		.cars__car-card
			picture
				if (car.srcx1)
					source(media="(min-width: 1921px)" srcset=car.srcx1)
				img.car-card__img(src=car.img alt=car.name title=car.name)
				//- else
				//- 	img.car-card__img(src=car.img alt=car.name title=car.name)
			.card-card__details
				p.details__price= car.price
				p.details__car-name= car.name
				ul.details__list
					li.details__item
