-
	var carsArr = [
		{img: 'img/cars/car1.png', price: '€40.00', name: 'Skoda Rapid `18', srcx1:'img/cars/retina/car_skoda.png'},
		{img: 'img/cars/car2.png', price: '€40.00', name: 'hyundai accent', srcx1: 'img/cars/retina/car_accent.png'}
